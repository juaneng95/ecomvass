<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Run unit tests" type="JUnit" factoryName="JUnit">
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.vass.inditex.application.controller.*" />
        <option name="ENABLED" value="true" />
      </pattern>
      <pattern>
        <option name="PATTERN" value="com.vass.inditex.domain.service.*" />
        <option name="ENABLED" value="true" />
      </pattern>
      <pattern>
        <option name="PATTERN" value="com.vass.inditex.infrastructure.service.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <option name="MAIN_CLASS_NAME" value="" />
    <option name="METHOD_NAME" value="" />
    <option name="TEST_OBJECT" value="pattern" />
    <option name="TEST_SEARCH_SCOPE">
      <value defaultName="wholeProject" />
    </option>
    <patterns>
      <pattern testClass="^(?!.*com\.vass\.inditex\.boot\.*).*$" />
    </patterns>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>